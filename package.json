    "url": "https://github.com/nestoralonso/srcmap-unpkg.git"
  },
  "keywords": [
    "srcmap",
    "source-map"
  ],
  "author": "NÃ©stor Alonso",
  "license": "MIT",
  "dependencies": {
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12"
  }
}
